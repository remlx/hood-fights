btns:Button(
    "fps unlocker",
    function()
local fps = 1000
 
    if setfpscap then
        setfpscap(fps)
end
    end
)

btns:Button(
    "rainbow character",
    function()
        for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("MeshPart") then
				v.Material = "ForceField"
				spawn(function()
					while wait() do
						for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
							if v:IsA("MeshPart") then
								v.Color = Color3.fromHSV(tick()%5/5,1,1)
								wait()
							end
						end 
					end
				end)
			end
		end
    end
)


